{
  "env": {
    "browser": true,
    "es2020": true
  },
  "rules": {
    "quotes": [2, "single", { "avoidEscape": true }]
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier", "prettier/react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  }
}
